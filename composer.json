{
    "name": "tmukherjee13/yii2-paypal",
    "description": "PayPal adaptive payment methods for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "paypal", "adaptive"],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Tarun Mukherjee",
        "email": "tmukherjee13@gmail.com"
    }],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "paypal/rest-api-sdk-php": "*",
        "paypal/adaptivepayments-sdk-php": "^3.9",
        "paypal/merchant-sdk-php": "*"
    },
    "autoload": {
        "psr-4": {
            "tmukherjee13\\paypal\\": "src"
        }
    },
    "extra": {
        "bootstrap": "tmukherjee13\\paypal\\PaymentBootstrap"
    }
}